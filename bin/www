#!/usr/bin/nodejs
var debug = require('debug')('my-application');
var app = require('../app');
var https = require('https');
var fs = require('fs');

var options = {
    key: fs.readFileSync('./certificates/pricegenie_key.txt'),
    cert: fs.readFileSync('./certificates/pricegenie_crt.txt')
};

https.createServer(options,app).listen(7012);